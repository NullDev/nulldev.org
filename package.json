{
  "name": "nulldev-org",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "description": "Source code for my website at nulldev.org",
  "scripts": {
    "start:dev": "bunx --bun vite",
    "start:build": "bunx --bun vite build",
    "start:preview": "bunx --bun vite preview",
    "blog:dev": "bunx --bun vitepress dev blog",
    "blog:build": "bunx --bun vitepress build blog",
    "blog:preview": "bunx --bun vitepress preview blog",
    "blog:new": "node .vitepress/theme/scripts/newPost.cjs",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "build": "npm run start:build && npm run blog:build"
  },
  "engines": {
    "node": ">=20.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NullDev/nulldev.org.git"
  },
  "keywords": [
    "NullDev",
    "Website",
    "Vite",
    "SCSS",
    "Terser",
    "ESLint",
    "LightningCSS",
    "VitePress",
    "TailwindCSS",
    "Markdown",
    "Vue"
  ],
  "author": "NullDev",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/NullDev/nulldev.org/issues"
  },
  "homepage": "https://nulldev.org/",
  "devDependencies": {
    "@heroicons/vue": "^2.0.18",
    "@babel/core": "^7.27.1",
    "@babel/eslint-parser": "^7.27.1",
    "@types/bun": "^1.2.12",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.26.0",
    "eslint-plugin-vue": "^10.1.0",
    "lightningcss": "^1.29.3",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-mathjax3": "^4.3.2",
    "prepend-file": "^2.0.1",
    "sass": "^1.87.0",
    "terser": "^5.39.0",
    "vite": "^6.3.5",
    "postcss": "8.4.35",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vitepress": "1.6.3",
    "vue": "^3.3.4"
  },
  "dependencies": {
    "function-plot": "^1.25.1"
  }
}
